@model TravelAgency.ViewModels.TicketViewModel

@{  
    ViewData["Title"] = "حجز التذاكر";
    TravelAgency.ViewModels.ReservedTickets seat;
    var seatNumber = 0;
}
<form asp-action="CreateNotAdmin" method="post">

  <div class="row">
   
      <div class="form-group col-3">
        <label asp-for="AppointmentId" class="control-label">الميعاد</label>
        <select asp-for="AppointmentId" class="form-control" asp-items="Model.AppointmentsList"></select>
      </div>

      <div class="form-group col-3">
        <label asp-for="TicketDate" class="control-label">يوم الحجز</label>
        <input data-seat-num="@seatNumber" asp-for="TicketDate" class="form-control" />

        <span asp-validation-for="TicketDate" class="text-danger"></span>
      </div>
      <div class="form-group   col-6" style="padding-top: 29px">
        <input data-seat-num="@seatNumber" type="submit" value="عرض الحجز"
               name="okshow" asp-action="ShowTickets" class="btn btn-primary" />
        &nbsp;
        <input type="submit" value="عرض مفصل"
               name="okshow" asp-action="MyTickets" class="btn btn-success" />
        &nbsp;    
        <label asp-for="CustomerId" class="control-label" style="background-color: darkgray">
          العميل
          <a id="Addclient" class="btn btn-outline-success btn-small">
            <span class="fa fa-plus">   </span>
          </a>
        </label>
      </div>
  </div>


  <hr />
  <div class="row" style="color:red;font-weight:bold">
    <div class="col-md-4">
      <br /> 


       <div class="form-group row">
    
      <div class="form-group col-6">
    <label  class="control-label">بحث عن عميل  
      <a   id="searchedCustomerId" class="btn btn-outline-success btn-small">  <span class="fa fa-plus"> </span></a> 
    </label>
                    <input type='number' class="form-control" id="ClientName" />
    </div>

    
  </div>

 <div class="form-group row">
      <div class="form-group col-6">
          <label asp-for="CustomerId" class="control-label">العميل     </label>
       <input type='hidden' asp-for="CustomerId"  class="form-control" id="CustomerIdadded" />
        <input type='text'    class="form-control" id="CustomerName" />
        </div>
  </div>

      <div class="form-group row">  
        <div class="form-group col-6">
          <label asp-for="SupplierId" class="control-label">المكتب   </label>
          <select asp-for="SupplierId" class="form-control  " asp-items="Model.SuppliersList"   style="width:100%"></select>
        </div>

        </div>
      <div class="form-group row">
        <div class="form-group col-md-6">
          <label asp-for="FromBranchId" class="control-label">مكتب المغادره</label>
          <select asp-for="FromBranchId" class="form-control" asp-items="Model.BranchsList"></select>
        </div>

        <div class="form-group col-md-6">
          <label asp-for="ToBranchId" class="control-label">مكتب الوصول</label>
          <select asp-for="ToBranchId" class="form-control" asp-items="Model.BranchsList"></select>
        </div>
      </div>
       
      <div class="form-group row">

        <div class="form-group  col-md-6">
          <label asp-for="SeatId" class="control-label">رقم المقعد</label>
          <input asp-for="SeatId" class="form-control" id="SeatNumber" value="0" />
          <span asp-validation-for="SeatId" class="text text-danger"></span>
        </div>

        <div class="form-group  col-md-6">
          <label asp-for="Price" class="control-label">السعر</label>
          <input asp-for="Price" class="form-control" id="price" />
          <span asp-validation-for="Price" class="text text-danger"></span>
        </div>
      </div>

      <div class="form-group row">

        <label asp-for="comment" class="control-label">ملاحظات</label>
        <textarea asp-for="comment" class="form-control"></textarea>

      </div>
      <div class="form-group  ">
        <input type="submit"
               value="حجز"
               class="btn btn-primary"
               id="doReserve" />
        <input type="hidden" class="form-control" id="ticketId"  asp-for="TicketId"  /> 
      </div> 
      <br /> 
      <div class="form-group  ">
        <div class="form-group col-6">
          <label asp-for="CustomerId" class="control-label">السعر </label>
          <input type="number" class="form-control" name="confirmPrice" id="confirmPrice"/>
        </div>
        <div class="form-group col-6">
          <label  class="control-label">المقعد     </label>
          <input type="text" class="form-control" id="ConfirmSeat"/>
        </div>
        <input asp-controller="Tickets"
               asp-action="DoConfirmReserve"
               type="submit"
               value="تأكيد الحجز"
               class="btn btn-warning"
               id="doConfirmReserve" />

      </div>
    </div>

    <div class=" col-sm-12  col-md-8  ">

      @*50*@
      <div class="row pt-1">
        <div class=" col-sm-1  col-md-1"> </div>
        @{
          seatNumber = 50;
          seat = Model.reservedTickets.FirstOrDefault(x => x.SeatId == seatNumber);

          if (seat == null)
          {
            <input data-seat-num="@seatNumber" type="button" class="btn btn-success col-md-2   col-sm-2 seat" value="@(seatNumber)" id="@seatNumber" style="height: 50px" />
          }
          else if (seat.IsMine)
          {
            <input data-seat-num="@seatNumber" type="button" class="@(seat.IsPaid ? "btn btn-danger col-md-2   col-sm-2  cancelTicket " : "btn btn-info col-md-2   col-sm-2  cancelTicket seat")"
                   data-IsPaid="@seat.IsPaid" data-Ticke-Id="@seat.TicketId" value="@(seatNumber)@(seat.Supplier)" id="@seatNumber" style="height: 50px">
          }
           else 
         {
            <input data-seat-num="@seatNumber" type="button" class="@(seat.IsPaid ? "btn btn-danger col-md-2   col-sm-2  cancelTicket " : "btn btn-info col-md-2   col-sm-2  cancelTicket seat")"
                   data-IsPaid="@seat.IsPaid" data-Ticke-Id="@seat.TicketId" value="@(seatNumber)" id="@seatNumber" style="height: 50px">
