@section Scripts
{
  @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
  <script type="text/javascript">
    $(document).ready(function() {

      $(".seat").click(function() {
        var seatNumber = $(this);
        var TextSeatNumber = $(this).attr("data-seat-num"); 
        var ticketId = $(this).attr("data-Ticke-Id");
        $("#ticketId").val(ticketId);
    
        if (seatNumber.hasClass("btn btn-info")) {
          $("#ConfirmSeat").val(TextSeatNumber);
          return;
        } 
          
        $(".seat").each(function() {
          var seat = $(this);
          if (seat.hasClass("btn btn-warning")) {
            seat.removeClass();
            seat.addClass("btn btn-success col-md-2   col-sm-2 seat");
          }
        });


        $("#SeatNumber").val(TextSeatNumber);
        seatNumber.removeClass();
        seatNumber.addClass("btn btn-warning col-md-2   col-sm-2 seat");

      });

      $("#ConfirmReserve").click(function() {
        var button = $(this);

        var price = document.getElementById('price').value;
        var ticketId = document.getElementById('ticketId').value;
       
        var result = confirm("هل تريد تأكيد التذكره بالفعل ؟");
        if (result) {
          $.ajax({
            url: '/Tickets/ConfirmTicket?id=' + ticketId + '&price=' + price,
            type: "POST",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function(data) {
              if (data === true) {
                button.removeClass();
                button.addClass("btn btn-danger  col-md-2 col-sm-2  seat");
              } else {
                alert("حدث خطأ اثناء تاكيد التذكره");
              }
            },
            error: function(result) {
              alert("Data is not in the right formate!! please contact with IT department");
            }
          })
        }
      });

      $("#Addclient").click(function() {
        $('#Modal_Accept').modal('show');
      });

      
