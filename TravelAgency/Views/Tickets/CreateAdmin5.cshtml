        <div class="modal-content">
            <div class="modal-header">

                <div class="  d-flex flex-column align-items-start pl-2">
                    <h4>اضافه عميل جديد</h4>
                </div>

                <button type="button" class="close " data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body">

                <div class="kt-portlet__body ">

                    <div class="form-group row">
                        <label class="col-form-label col-lg-3 col-sm-12">الاسم </label>
                        <div class="col-lg-9 col-md-9 col-sm-12">
                            <input type="text" class="form-control " id="cname" />
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-form-label col-lg-3 col-sm-12">الهاتف </label>
                        <div class="col-lg-9 col-md-9 col-sm-12">
                            <input type="number" class="form-control " maxlength=12 id="cphone" />
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-form-label col-lg-3 col-sm-12">البلد </label>
                        <div class="col-lg-9 col-md-9 col-sm-12">
                            <select class="form-control" id="cAdreess" required>
                                <option selected value=""> </option>
                                <option value="الوقف">الوقف</option>
                                <option value="المراشده">المراشده</option>
                                <option value="المنشيه">المنشيه</option>
                                <option value="الترامسه">الترامسه</option>
                                <option value="قنا">قنا</option>
                            </select>
                        </div>
                    </div>

                </div>
                <div class="  clearfix p-0 pt-3">
                    <div class=" ">
                        <button class="btn btn-primary pull-right" id="doAddClient">حفظ</button>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts
{
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    <script type="text/javascript">
        $(document).ready(function () {

            $(".seat").click(function () {
                var selectedSeat = $(this);
                var seatNum = $(this).attr("data-seat-num");

                resetSeatColor()

                $("#SeatNumber").val(seatNum);
                selectedSeat.removeClass();
                selectedSeat.addClass("btn btn-warning  col-md-2 col-sm-2  seat");

            });

            $(".reservedSeat").click(function () {
                var selectedSeat = $(this);
                var seatNum = $(this).attr("data-seat-num");
                var tickeId = $(this).attr("data-Ticke-Id");
                var PhoneNumber = $(this).attr("data-PhoneNumber");
                var customerCode = $(this).attr("data-cust-code");

                resetSeatColor()
                document.getElementById("call").href = "tel:" + PhoneNumber;

                $("#SeatNumber").val(seatNum);
                $("#DeleteTicketId").val(tickeId);
                $("#ClientCode").val(customerCode);

                selectedSeat.removeClass();
                selectedSeat.addClass("btn btn-warning  col-md-2 col-sm-2  reservedSeat");

                var ticketNumber = document.getElementById('DeleteTicketId');
                $.ajax({
                    url: '/Tickets/GetCustomerInfo?id=' + ticketNumber.value,
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (data) {
                        console.log(data);
                        $("#ClientPhone").val(data.split("&&")[0]);
                        document.getElementById("SupplierId").value = data.split("&&")[1];
                        $("#CustomerIdAdded").val(data.split("&&")[2]);
                        $("#CustomerName").val(data.split("&&")[3]);
                        $("#CustomerPoints").val(data.split("&&")[4]);
                    },
                    error: function (result) {
                        alert("Data is not in the right formate!! please contact with IT department");
                    }
                })

            });

            $(".cancelTicket").click(function () {

                var button = $(this);
                var TicketNumber = button.attr("data-Ticke-Id");

                var result = confirm("هل تريد حذف التذكره بالفعل ؟");
                if (result) {
                    $.ajax({
                        url: '/Tickets/DeleteTicket?id=' + TicketNumber,
                        type: "POST",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (data) {
                            if (data === true) {
                                button.removeClass();
                                button.addClass("btn btn-success  col-md-2 col-sm-2  seat");
                                button.val(button[0].id);
                            } else {
                                alert("حدث خطأ اثناء استرجاع التذكره");
                            }
                        },
                        error: function (result) {
                            alert("Data is not in the right formate!! please contact with IT department");
                        }
                    })
                }
            });

            $("#doDeleteSeat").click(function () {

                var ticketNumber = document.getElementById('DeleteTicketId');

                var result = confirm("هل تريد حذف التذكره بالفعل ؟");
                if (result) {
                    $.ajax({
                        url: '/Tickets/DeleteTicket?id=' + ticketNumber.value,
                        type: "POST",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (data) {
                            if (data === true) {
                                $("#SeatNumber").val();
                                resetSeatColorAfterDelet();
                            } else {
                                alert("حدث خطأ اثناء استرجاع التذكره");
                            }
                        },
                        error: function (result) {
                            alert("Data is not in the right formate!! please contact with IT department");
                        }
                    })
                }
            });

            $("#AddClient").click(function () {
                var PhoneNumber = document.getElementById('ClientPhone');
                var customerName = document.getElementById('CustomerName');
                $("#cphone").val(PhoneNumber.value);
                $("#cname").val(CustomerName.value);
                $('#Modal_Accept').modal('show');
            });

            $("#doAddClient").click(function () {
                var name = document.getElementById('cname').value;
                var phone = document.getElementById('cphone').value;
                var adreess = document.getElementById('cAdreess').value;

                $.ajax({
                    url: '/Tickets/AddCustomerAdmin',
                    type: "POST",
                    data: { name: name, phone: phone, Adreess1: adreess },
                    contentType: "application/x-www-form-urlencoded; charset=UTF-8",
                    dataType: "json",
                    success: function (data) {
                        if (data === true) {
                            $("#phone").val(phone);
                            $('#Modal_Accept').modal('hide');
                        } else {

                        }
                    },
                    error: function (xhr, status, error) {

                        alert("Data is not in the right format!! Please contact the IT department.");
                    }
                });
            });

            $("#ClientPhone").keyup(function () {
                var PhoneNumber = document.getElementById('ClientPhone');

                $.ajax({
                    url: '/Tickets/FindCustomerByPhoneId?Phone=' + PhoneNumber.value,
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (data) {
                        $("#CustomerIdAdded").val(data.split("&&")[0]);
                        $("#CustomerName").val(data.split("&&")[1]);
                        $("#ClientCode").val(data.split("&&")[3]);
                        $("#CustomerPoints").val(data.split("&&")[4]);

                    },
                    error: function (result) {
                        alert("Data is not in the right formate!! please contact with IT department");
                    }
                })
            });

            $("#ClientCode").keyup(function () {
                var PhoneNumber = document.getElementById('ClientCode');

                $.ajax({
                    url: '/Tickets/FindCustomerByCode?code=' + PhoneNumber.value,
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (data) {
                        $("#CustomerIdAdded").val(data.split("&&")[0]);
                        $("#CustomerName").val(data.split("&&")[1]);
                        $("#ClientPhone").val(data.split("&&")[2]);
                        $("#CustomerPoints").val(data.split("&&")[3]);
                         
                    },
                    error: function (result) {
                        alert("Data is not in the right formate!! please contact with IT department");
                    }
                })
            });

            $("#searchedCustomerId").click(function () {

                var PhoneNumber = document.getElementById('ClientPhone');

                $.ajax({
                    url: '/Tickets/FindCustomerByPhoneId?Phone=' + PhoneNumber.value,
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (data) {

                        $("#CustomerIdAdded").val(data.split("&&")[0]);
                        $("#CustomerName").val(data.split("&&")[1]);
                      

                    },
                    error: function (result) {
                        alert("Data is not in the right formate!! please contact with IT department");
                    }
                })
            });

            function resetSeatColor() {

                $(".seat").each(function () {
                    var seat = $(this);
                    if (seat.hasClass("btn btn-warning")) {
                        seat.removeClass();
                        seat.addClass("btn btn-success  col-md-2 col-sm-2  seat");
                    }
                });

                $(".reservedSeat").each(function () {
                    var seat = $(this);
                    if (seat.hasClass("btn btn-warning")) {
                        seat.removeClass();
                        seat.addClass("btn btn-info  col-md-2 col-sm-2  reservedSeat");
                    }
                });
            }

            function resetSeatColorAfterDelet() {
                $(".reservedSeat").each(function () {
                    var seat = $(this);
                    if (seat.hasClass("btn btn-warning")) {
                        seat.removeClass();
                        seat.addClass("btn btn-success  col-md-2 col-sm-2  seat");

                    }
                });
            }
        });
    </script>
