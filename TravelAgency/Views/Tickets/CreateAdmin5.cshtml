@model TravelAgency.ViewModels.TicketViewModel

@{
    ViewData["Title"] = "حجز التذاكر";
    TravelAgency.ViewModels.ReservedTickets seat;
    var seatNumber = 0;
}
<form asp-action="CreateAdmin">

    <div class="row">
        <div class="form-group col-sm-2">
            <label asp-for="AppointmentId" class="control-label">الميعاد</label>
            <select asp-for="AppointmentId" class="form-control" asp-items="Model.AppointmentsList"></select>
        </div>

        <div class="form-group col-sm-3">
            <label asp-for="TicketDate" class="control-label">يوم الحجز</label>
            <input data-seat-num="@seatNumber" asp-for="TicketDate" class="form-control" />
            <span asp-validation-for="TicketDate" class="text-danger"></span>
        </div>

        <div class="form-group row col-4" style="height:40PX ;margin-top: 20PX">
            <input  type="submit" value="عرض الحجز"  class="btn btn-primary" asp-action="ShowTicketsForAdmin" />
          @*   &nbsp; &nbsp;
            <input type="submit" value="عرض مفصل" asp-action="MyTickets" class="btn btn-info" /> *@
        </div>

    </div>
    <hr />
    <div class="row">

        <div class="form-group col-sm-2"><input type='number' placeholder="الهاتف" class="form-control" id="ClientPhone"/></div>
        <div class="form-group col-sm-2"><input type='number' placeholder="الكود" class="form-control" id="ClientCode"/></div>
        <div class="form-group"> <a class="btn btn-outline-success btn-small" id="searchedCustomerId"> <span class="fa fa-search"></span></a></div>
        <div class="form-group col-sm-2"><input type='text' placeholder="الاسم" class="form-control" id="CustomerName"/></div> 
        <div class="form-group"> <a class="btn btn-outline-success btn-small" id="AddClient"> <span class="fa fa-plus"></span></a></div>
        <div class="form-group col-sm-2"><input type='text' placeholder="النقاط" class="form-control" id="CustomerPoints" disabled/> </div> 
    </div>

    <div class="row">
        <div class="form-group  col-sm-2"><select asp-for="SupplierId" id="SupplierId" class="form-control " asp-items="Model.SuppliersList" style="width:100%"></select>  </div>

        <span class="form-group" style=" margin-top: 10PX">  <label class="control-label">ســـت </label> </span>

        <div class="form-group  col-sm-1">    <input asp-for="IsFemale" class="form-control" />   </div>

        @* <div class="form-group" style=" margin-top: 10PX">  <label class="control-label">سعر</label> </div>

        <div class="form-group  col-sm-2">
            <input asp-for="Price" class="form-control" value="0" />
            <span asp-validation-for="Price" class="text text-danger"></span>
        </div> *@

        <div class="form-group" style=" margin-top: 10PX">  <label class="control-label">مقعد</label> </div>

        <div class="form-group  col-sm-2">
            <input asp-for="SeatId" class="form-control" id="SeatNumber" value="0" />
            <span asp-validation-for="SeatId" class="text text-danger"></span> 
        </div>
         
        <div class="form-group ">
            <input type='hidden' asp-for="CustomerId" class="form-control" id="CustomerIdAdded" />
            <input type='hidden' class="form-control" id="DeleteTicketId" />

            <input type="submit" value="تاكيد الحجز" asp-action="createAdmin" class="btn btn-primary" /> 
            <a id="doDeleteSeat" class="btn btn-danger"> <i class="fa fa-trash"></i> حذف الحجز </a>
            <a id="call" class="btn btn-success"> <i style="color: white" class="fa fa-phone"></i> اتصال </a> 
        </div>

    </div>
    <hr />

    <div class="row" style="font-weight:bold">

        <div class=" col-sm-12  col-md-12   ">
            @*50*@
            <div class="row pt-1">
                <div class=" col-sm-1  col-md-1"> </div>
                @{
                    seatNumber = 50;
                    seat = Model.reservedTickets.FirstOrDefault(x => x.SeatId == seatNumber);

                    if (seat == null)
                    {
                        <input data-seat-num="@seatNumber" type="button" class="btn btn-success  col-md-2 col-sm-2  seat" title="متاح" value="@(seatNumber)" id="@seatNumber" style="height: 100px" />
                    }

                    else
                    {
                        <button data-seat-num="@seatNumber" type="button" class="@(seat.IsFemale ? "btn btn-danger  col-md-2 col-sm-2   reservedSeat" : "btn btn-info  col-md-2 col-sm-2 reservedSeat")"
                                data-IsFemale="@seat.IsFemale" data-Ticke-Id="@seat.TicketId"  data-cust-code="@seat.Code"  data-PhoneNumber="@seat.Phone" id="@seatNumber" style="height: 100px">
                             @(seatNumber)-@(seat.Supplier) <br /> <span class="@(seat.Price !=0 ? "label label-warning bold" : "")"> @(seat.Customer) </span><br /> @(seat.Phone)
                        </button>
                    }
