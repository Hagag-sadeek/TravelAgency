@model TravelAgency.ViewModels.TicketViewModel

@{
    ViewData["Title"] = "حجز التذاكر";
    TravelAgency.ViewModels.ReservedTickets seat;
    var seatNumber = 0;
}
<form asp-action="CreateAdmin">

    <div class="row">
        <div class="form-group col-sm-4">
            <label asp-for="AppointmentId" class="control-label">الميعاد</label>
            <select asp-for="AppointmentId" class="form-control" asp-items="Model.AppointmentsList"></select>
        </div>

        <div class="form-group col-sm-3">
            <label asp-for="TicketDate" class="control-label">يوم الحجز</label>
            <input data-seat-num="@seatNumber" asp-for="TicketDate" class="form-control" />

            <span asp-validation-for="TicketDate" class="text-danger"></span>
        </div>

    </div>

    <div class="form-group row col-12">
        <input data-seat-num="@seatNumber"
               type="submit"
               value="عرض الحجز"
               name="okshow"
               asp-action="ShowTicketsForAdmin"
               class="btn btn-primary" />
        <input type="submit" value="عرض مفصل"
               asp-action="MyTickets" class="btn btn-info" />
    </div>

    <hr />
    <div class="row" style="color:red;font-weight:bold">

        <div class="col-md-4">

            <div class="form-group">

                <label class="control-label btn btn-outline-success " id="searchedCustomerId">
                    بحث عن عميل

                </label>
                <input type='text' class="form-control" id="ClientName" />

            </div>
            <br />

            <div class="form-group">

                <label asp-for="CustomerId" class="control-label">
                    العميل
                    <a id="Addclient" class="btn btn-outline-success btn-small">
                        <span class="fa fa-plus">   </span>
                    </a>
                </label>
                <input type='hidden' asp-for="CustomerId" class="form-control" id="CustomerIdadded" />
                <input type='text' class="form-control" id="CustomerName" />
                @*   <select asp-for="CustomerId" id="CustomerIdList" class="form-control select2" asp-items="Model.CustomersList" style="width:100%"></select>*@
            </div>
            <br />

            <div class="form-group">
                <label asp-for="SupplierId" class="control-label">المكتب <a asp-controller="Suppliers" asp-action="create" class="btn btn-outline-success btn-small"><span class="fa fa-plus">   </span></a> </label>
                <select asp-for="SupplierId" class="form-control " asp-items="Model.SuppliersList" style="width:100%"></select>
            </div>

            @*   <div class="form-group row">
            <div class="form-group  col-md-6">
            <label asp-for="FromBranchId" class="control-label">مكتب المغادره</label>
            <select asp-for="FromBranchId" class="form-control" asp-items="Model.BranchsList"></select>
            </div>


            <div class="form-group col-md-6">
            <label asp-for="ToBranchId" class="control-label">مكتب الوصول</label>
            <select asp-for="ToBranchId" class="form-control" asp-items="Model.BranchsList"></select>
            </div>
            </div> *@

            <div class="form-group row">
                <div class="form-group  col-md-6">
                    <label asp-for="Price" class="control-label">سعر التذكره</label>
                    <input data-seat-num="@seatNumber" asp-for="Price" class="form-control" value="0" />
                    <span asp-validation-for="Price" class="text text-danger"></span>
                </div>
                <div class="form-group  col-md-6">
                    <label asp-for="SeatId" class="control-label">رقم المقعد</label>
                    <input data-seat-num="@seatNumber" asp-for="SeatId" class="form-control" id="SeatNumber" value="0" />
                    <span asp-validation-for="SeatId" class="text text-danger"></span>
                </div>

            </div>
            <div class="form-group  ">
                <input data-seat-num="@seatNumber"
                       type="submit"
                       value="تـأكيــد الحـجـز"
                       asp-action="createAdmin"
                       class="btn btn-primary"
                       id="doReserve" />

            </div>
        </div>

        <div class=" col-sm-12  col-md-8  ">
            @*50*@
            <div class="row pt-1">
                <div class=" col-sm-1  col-md-1"> </div>
                @{
                    seatNumber = 50;
                    seat = Model.reservedTickets.FirstOrDefault(x => x.SeatId == seatNumber);

                    if (seat == null)
                    {
                        <input data-seat-num="@seatNumber" type="button" class="btn btn-success  col-md-2 col-sm-2  seat" title="متاح" value="@(seatNumber)" id="@seatNumber" style="height: 50px" />
                    }
                    else if (!seat.IsPaid)
                    {
                        <input data-seat-num="@seatNumber" type="button" class="btn btn-info  col-md-2 col-sm-2  seat cancelTicket"
                               data-IsPaid="@seat.IsPaid" data-Ticke-Id="@seat.TicketId" value="@(seatNumber)@(seat.Supplier)" id="@seatNumber" style="height: 50px">
                    }
                    else
                    {
                        <input data-seat-num="@seatNumber" type="button" class="@(seat.IsPaid ? "btn btn-danger  col-md-2 col-sm-2   cancelTicket " : "btn btn-info  col-md-2 col-sm-2   cancelTicket")"
                               data-IsPaid="@seat.IsPaid" data-Ticke-Id="@seat.TicketId" value="@(seatNumber)@(seat.Supplier)" id="@seatNumber" style="height: 50px">
                    }
