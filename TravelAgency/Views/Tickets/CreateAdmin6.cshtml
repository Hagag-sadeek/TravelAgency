<div id="Modal_Accept" class="modal fade" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">

                <div class="  d-flex flex-column align-items-start pl-2">
                    <h4>اضافه عميل جديد</h4>
                </div>

                <button type="button" class="close " data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form class="Acceptform" method="post" asp-action="AddCustomerAdmin" asp-controller="tickets">
                    <div class="kt-portlet__body ">

                        <div class="form-group row">
                            <label class="col-form-label col-lg-3 col-sm-12">الاسم </label>
                            <div class="col-lg-9 col-md-9 col-sm-12">
                                <input type="text" class="form-control " name="name" />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-form-label col-lg-3 col-sm-12">الهاتف </label>
                            <div class="col-lg-9 col-md-9 col-sm-12">
                                <input type="number" class="form-control " maxlength=12 name="phone" />
                            </div>
                        </div>
                    </div>
                    <div class="  clearfix p-0 pt-3">
                        <div class=" ">
                            <button class="btn btn-primary pull-right">حفظ</button>

                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

@section Scripts
{
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    <script type="text/javascript">
        $(document).ready(function () {
            $(".seat").click(function () {
                var seatNumber = $(this);
                var textseatnumber = $(this).attr("data-seat-num");

                if (seatNumber.hasClass("btn btn-info")) {
                    // $("#SeatNumber").val(textseatnumber);
                    return;
                }

                $(".seat").each(function () {
                    var seat = $(this);
                    if (seat.hasClass("btn btn-warning")) {
                        seat.removeClass();
                        seat.addClass("btn btn-success  col-md-2 col-sm-2  seat");
                    }
                });


                $("#SeatNumber").val(textseatnumber);
                seatNumber.removeClass();
                seatNumber.addClass("btn btn-warning  col-md-2 col-sm-2  seat");

            });

            $(".cancelTicket").click(function () {

                var button = $(this);
                var TicketNumber = button.attr("data-Ticke-Id");

                var result = confirm("هل تريد حذف التذكره بالفعل ؟");
                if (result) {
                    $.ajax({
                        url: '/Tickets/DeleteTicket?id=' + TicketNumber,
                        type: "POST",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (data) {
                            if (data === true) {
                                button.removeClass();
                                button.addClass("btn btn-success  col-md-2 col-sm-2  seat");
                                button.val(button[0].id);
                            } else {
                                alert("حدث خطأ اثناء استرجاع التذكره");
                            }
                        },
                        error: function (result) {
                            alert("Data is not in the right formate!! please contact with IT department");
                        }
                    })
                }
            });


            $("#Addclient").click(function () {
                $('#Modal_Accept').modal('show');
            });

            $("#ClientName").keyup(function () {
                var PhoneNumber = document.getElementById('ClientName');

                $.ajax({
                    url: '/Tickets/FindCustomerByPhoneId?Phone=' + PhoneNumber.value,
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (data) {
                        $("#CustomerIdadded").val(data.split("&&")[0]);
                        $("#CustomerName").val(data.split("&&")[1]);

                    },
                    error: function (result) {
                        alert("Data is not in the right formate!! please contact with IT department");
                    }
                })
            });

            $("#searchedCustomerId").click(function () {

                var PhoneNumber = document.getElementById('ClientName');

                $.ajax({
                    url: '/Tickets/FindCustomerByPhoneId?Phone=' + PhoneNumber.value,
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (data) {

                        $("#CustomerIdadded").val(data.split("&&")[0]);
                        $("#CustomerName").val(data.split("&&")[1]);

                    },
                    error: function (result) {
                        alert("Data is not in the right formate!! please contact with IT department");
                    }
                })
            });



        });
    </script>
}